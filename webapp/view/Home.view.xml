<mvc:View controllerName='ui5.template.controller.Home' displayBlock='true'
  xmlns='sap.m'
  xmlns:mvc='sap.ui.core.mvc'>
  <Page title='Challenges List'>
    <List items='{state>/pages}'>
      <StandardListItem title='{state>name}' type='Active' press='onGoToChallenge(${state>id}, ${state>name})'/>
    </List>
  </Page>
</mvc:View>
